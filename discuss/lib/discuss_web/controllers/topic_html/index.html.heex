<h5> Topics </h5>

<ul class="collection">
    <%= for topic <- @topics do %>
        <li class="collection-item">
         <%= topic.title %>
        </li>
    <% end %>
</ul>

<div class="fixed-action-btn">
    <%= live_patch to: DiscussWeb.Router.Helpers.topic_path(@conn, :new), class: "btn-floation btn-large waves-effect waves-light red" do %>
        <i class="material-icons">add</i>
    <% end %>
</div>
